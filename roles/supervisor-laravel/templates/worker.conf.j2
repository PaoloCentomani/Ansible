[program:{{ domain_name }}-worker]
process_name=%(program_name)s_%(process_num)02d
command=php /var/www/{{ domain_name }}/current/artisan queue:work database --sleep=3 --tries=3 --queue=priority,default
autostart=true
autorestart=true
user=www-data
numprocs=2
redirect_stderr=true
stdout_logfile=/var/www/{{ domain_name }}/shared/storage/logs/worker.log
